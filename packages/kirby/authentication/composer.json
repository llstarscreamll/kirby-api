{
    "name": "kirby/authentication",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Alvarez",
            "email": "llstarscreamll@hotmail.com",
            "homepage": "https://github.com/llstarscreamll"
        }
    ],
    "homepage": "https://github.com/llstarscreamll/authentication",
    "keywords": [
        "Kirby",
        "Authentication"
    ],
    "require": {
        "illuminate/support": "~6",
        "laravel/passport": "~7",
        "barryvdh/laravel-cors": "^0.11.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Kirby\\Authentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kirby\\Authentication\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kirby\\Authentication\\AuthenticationServiceProvider"
            ],
            "aliases": {
                "Authentication": "Kirby\\Authentication\\Facades\\Authentication"
            }
        }
    }
}
