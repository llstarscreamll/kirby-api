{
    "name": "kirby/products",
    "description": "products API",
    "license": "license",
    "authors": [
        {
            "name": "Johan alvarez",
            "email": "llstarscreamll@hotmail.com",
            "homepage": "https://github.com/llstarscreamll"
        }
    ],
    "homepage": "https://github.com/kirby/products",
    "keywords": [
        "Laravel",
        "products"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "spatie/laravel-fractal": "^5.7",
        "spatie/laravel-query-builder": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Kirby\\Products\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kirby\\Products\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kirby\\Products\\ProductsServiceProvider"
            ],
            "aliases": {
                "Products": "Kirby\\Products\\Facades\\Products"
            }
        }
    }
}
