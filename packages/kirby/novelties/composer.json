{
    "name": "kirby/novelties",
    "description": ":description",
    "license": "license",
    "authors": [
        {
            "name": "Johan Alvarez",
            "email": "llstarscreamll@hotmail.com",
            "homepage": "https://github.com/llstarscreamll"
        }
    ],
    "homepage": "https://github.com/llstarscreamll/novelties",
    "keywords": [
        "Laravel",
        "Novelties"
    ],
    "require": {
        "bensampo/laravel-enum": "^1.19",
        "illuminate/support": "~7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Kirby\\Novelties\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kirby\\Novelties\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kirby\\Novelties\\NoveltiesServiceProvider"
            ],
            "aliases": {
                "Novelties": "Kirby\\Novelties\\Facades\\Novelties"
            }
        }
    }
}
