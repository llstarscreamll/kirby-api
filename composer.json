{
    "name": "llstarscreamll/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": {
        "kirby/*": {
            "type": "path",
            "url": "./packages/kirby/*"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-redis": "*",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "kirby/authentication": "*",
        "kirby/authorization": "*",
        "kirby/company": "*",
        "kirby/core": "*",
        "kirby/customers": "*",
        "kirby/employees": "*",
        "kirby/machines": "*",
        "kirby/novelties": "*",
        "kirby/production": "*",
        "kirby/products": "*",
        "kirby/time-clock": "*",
        "kirby/users": "*",
        "kirby/work-shifts": "*",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4",
        "laravel/telescope": "^3",
        "laravel/ui": "^2.0",
        "lcobucci/jwt": "3.3.2",
        "maatwebsite/excel": "^3.1",
        "nunomaduro/collision": "^4.1",
        "spatie/data-transfer-object": "^2.6",
        "spatie/laravel-backup": "6.*",
        "spatie/period": "1.5.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "blastcloud/guzzler": "^1.5",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jeroen-g/laravel-packager": "^2.1",
        "laravel/tinker": "^2.0",
        "makeabledk/laravel-factory-enhanced": "^2",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.5",
        "phpunit/php-code-coverage": "^7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
